
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option type 'bridge'
	option ifname 'eth1.1'
	option proto 'dhcp'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option vid '1'
	option ports '0t 1 2 3 4 5 6t'

config interface 'LAN6'
	option proto 'dhcpv6'
	option ifname 'br-lan'
	option reqaddress 'try'
	option reqprefix 'auto'

config interface 'LAN6CAFE'
	option proto 'static'
	option ifname 'br-lan'
	list ip6addr '2001:985:502d:1:c:a:f:e'
	option ip6gw '2001:985:502d:1::'

